                                li
                                    i.icon-ok.text--success.icon-2x
                                li
                                    i.icon-ok.text--success.icon-2x
                                li 60 дней
                        .plan__tier-footer
                            h2 22 846 Р
                            p.reasons__text
                                b.text--md + скидка
                                span  5600 Р на создание
                                span  Landing Page
                            a.btn.btn--success.margtop20(href="#") Заказать
                section.col-md-3.col-sm-6.col-xs-12
                    .plan__tier.text--center.margleft10.margright10
                        .plan__tier-header
                            h5.text--uppercase Профессиональный
                        .plan__tier-body
                            ul.list--unstyled
                                li Яндекс и Google
                                li До 1 000
                                li
                                    i.icon-cancel.text--danger.icon-2x
